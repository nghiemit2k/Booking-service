{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/booking/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/booking/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/booking",
  "projects": {
    "@apps/booking": {
      "type": "application",
      "root": "apps/booking",
      "entryFile": "main",
      "sourceRoot": "apps/booking/src",
      "compilerOptions": {
        "tsConfigPath": "apps/booking/tsconfig.app.json"
      }
    },
    "@apps/webhook-service": {
      "type": "application",
      "root": "apps/webhook-service",
      "entryFile": "main",
      "sourceRoot": "apps/webhook-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/webhook-service/tsconfig.app.json"
      }
    },
    "@libs/core": {
      "type": "library",
      "root": "libs/@libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/@libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/@libs/core/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    }
  }
}